{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "lib": ["esnext"],
    "jsx": "react-jsx",
    "strict": true,
    "noImplicitOverride": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": false,
    "skipLibCheck": true
  },
  "include": ["**/*"]
}
