{
  "name": "@hatena/eslint-config-hatena",
  "version": "1.0.0",
  "description": "ESLint config for @hatena",
  "main": "index.js",
  "repository": "https://github.com/hatena/eslint-config-hatena",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "peerDependencies": {
    "eslint": ">=6.8.0",
    "prettier": ">=2.0.5",
    "typescript": ">=3.9.3"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "autoInstall": false,
      "optional": false
    },
    "prettier": {
      "autoInstall": false,
      "optional": true
    },
    "typescript": {
      "autoInstall": false,
      "optional": true
    }
  },
  "devDependencies": {
    "@hatena/prettier-config-hatena": "https://github.com/hatena/prettier-config-hatena.git#v1.0.0",
    "eslint": "^8.57.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  },
  "files": [
    "*.js"
  ]
}
